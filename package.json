{
  "name": "openapi-typegen",
  "version": "0.0.0-development",
  "description": "Generate typescript from swagger files",
  "main": "./lib/core/index.js",
  "bin": {
    "typgen": "./lib/cli/index.js"
  },
  "repository": "git@github.com:TobiasWalle/swagger-typegen.git",
  "author": "Tobias Walle",
  "license": "MIT",
  "private": false,
  "scripts": {
    "cli": "ts-node src/cli/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "tslint -p tsconfig.json"
  },
  "devDependencies": {
    "@types/args": "^3.0.0",
    "@types/fs-extra": "^5.0.1",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "@types/ora": "^1.3.3",
    "axios": "^0.18.0",
    "coveralls": "^3.0.0",
    "jest": "^22.4.2",
    "open-api.d.ts": "^3.0.1",
    "ts-jest": "^22.4.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.0-rc"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "args": "^3.0.8",
    "ora": "^2.0.0",
    "ts-simple-ast": "^9.3.0"
  }
}
