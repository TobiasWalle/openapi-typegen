{
  "name": "swagger-typegen",
  "version": "0.0.0",
  "description": "Generate typescript from swagger files",
  "main": "index.js",
  "repository": "git@github.com:TobiasWalle/swagger-typegen.git",
  "author": "Tobias Walle",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.1",
    "typescript": "^2.8.0-rc"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
