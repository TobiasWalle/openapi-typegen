{
  "name": "openapi-typegen",
  "version": "0.0.0-development",
  "description": "Generate typescript from swagger files",
  "main": "./lib/core/index.js",
  "files": [
    "lib"
  ],
  "bin": {
    "typegen": "./lib/cli/index.js"
  },
  "repository": "git@github.com:TobiasWalle/openapi-typegen.git",
  "author": "Tobias Walle",
  "license": "MIT",
  "private": false,
  "scripts": {
    "cli": "ts-node src/cli/index.ts",
    "run-example": "yarn cli --src './examples/pet-store/schema.json' --out ./generated",
    "build": "tsc",
    "prepublish": "run-s lint test build",
    "test": "jest",
    "lint": "tslint -p tsconfig.json",
    "semantic-release": "semantic-release",
    "coveralls": "cat coverage/lcov.info | node node_modules/.bin/coveralls",
    "travis-deploy-once": "travis-deploy-once"
  },
  "devDependencies": {
    "@types/args": "^3.0.0",
    "@types/fs-extra": "^5.0.1",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "@types/ora": "^1.3.3",
    "axios": "^0.18.0",
    "coveralls": "^3.0.0",
    "jest": "^22.4.2",
    "npm-run-all": "^4.1.2",
    "open-api.d.ts": "^3.0.1",
    "semantic-release": "^15.1.4",
    "ts-jest": "^22.4.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.0-rc",
    "travis-deploy-once": "^4.4.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "args": "^3.0.8",
    "ora": "^2.0.0",
    "ts-simple-ast": "^9.3.0"
  }
}
