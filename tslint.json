{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "quotemark": [true, "single"],
        "arrow-parens": false,
        "interface-name": false,
        "object-literal-sort-keys": false,
        "array-type": false,
        "trailing-comma": false
    },
    "rulesDirectory": []
}